//- footer.pug
	The template for displaying the footer

	Contains the closing of the #content div and all content after

footer.site-footer#colophon(role="contentinfo")
	.constrain
		span.res
		include template-parts/footer/_#footer-widgets.pug
		include template-parts/footer/_#site-info.pug
include template-parts/ui/_#page-back-to-top.pug
include template-parts/ui/_#page-scroll-indicator.pug
script(type="text/javascript" src='assets/js/app.js')
script(type="text/javascript" src='assets/js/wp.js')
//if lte IE 8
	script(src='assets/js/ie/respond.min.js